        <script>
            /**
             * This script is used to clear local storage when the version of the app is changed.
             * This is useful when we have some breaking changes in the app and we want to clear the local storage.
             * CI/CD will replace 1ab9aa9c with the current version of the app.
             */
            if (localStorage.getItem("f8:version") !== "1ab9aa9c") {
                localStorage.clear();
                localStorage.setItem("f8:version", "1ab9aa9c");
            }

            /**
             * Redirect to the new domain if the app is running on localhost.
             */
            if (location.hostname === "localhost") {
                location.replace("https://f8team.local2");
            }
        </script>

